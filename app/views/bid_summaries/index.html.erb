<h1>Listing bid_summaries</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Return student</th>
    <th>New student</th>
    <th>Open all</th>
    <th>Faculty</th>
    <th>Quota</th>
    <th>Bidders</th>
    <th>Lowest bid</th>
    <th>Highest bid</th>
    <th>Winning bid</th>
    <th>Academic year</th>
    <th>Round</th>
    <th>Lecture group</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @bid_summaries.each do |bid_summary| %>
  <tr>
    <td><%= bid_summary.id %></td>
    <td><%= bid_summary.return_student %></td>
    <td><%= bid_summary.new_student %></td>
    <td><%= bid_summary.open_all %></td>
    <td><%= bid_summary.faculty %></td>
    <td><%= bid_summary.quota %></td>
    <td><%= bid_summary.bidders %></td>
    <td><%= bid_summary.lowest_bid %></td>
    <td><%= bid_summary.highest_bid %></td>
    <td><%= bid_summary.winning_bid %></td>
    <td><%= bid_summary.academic_year %></td>
    <td><%= bid_summary.round %></td>
    <td><%= bid_summary.lecture_group %></td>
    <td><%= link_to 'Show', bid_summary %></td>
    <td><%= link_to 'Edit', edit_bid_summary_path(bid_summary) %></td>
    <td><%= link_to 'Destroy', bid_summary, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

